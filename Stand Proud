local Load_Key = loadstring(game:HttpGet("https://raw.githubusercontent.com/SmellLikeHacker/IDK/main/Key"))()
local key
local vu = game:GetService("VirtualUser")
game:GetService("Players").LocalPlayer.Idled:connect(function()
   vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
   wait(1)
   vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
end)
current_key = nil

if current_key == "nil" then
    current_key = nil
end

if current_key ~= nil then
if isfile("!KeyPurpleCat.txt") and readfile("!KeyPurpleCat.txt") == current_key then
    
key = Load_Key(nil)

passkey = true

else
key = Load_Key(current_key , function()
    writefile("!KeyPurpleCat.txt" , current_key)
    

    passkey = true
end)
end
else
    key = Load_Key(nil)
    key.Text = "Loading . . ."
    passkey = true
end

wait(.2)

repeat wait() until passkey == true
    
    key.Text = "Key is Correct!"
    
    wait(1)
    
    game.CoreGui:FindFirstChild("PurpleCat"):Destroy()

local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/SmellLikeHacker/IDK/main/GUI56"))()
local CenterHubNo1 = library:CreateWindow(game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name,Enum.KeyCode.RightControl)
local Tab = CenterHubNo1:CreateTab("Main")
local Sector1 = Tab:CreateSector("Main","left")
Sector1:AddLabel("Auto Farm")
Sector1:AddToggle("Auto Give Money",false,function(t)
   _G.AutoGiveMoney = t
end)

spawn(function()
pcall(function()
while wait() do
if _G.AutoGiveMoney then
pcall(function()
game:GetService("ReplicatedStorage").Remotes.AquireReward:FireServer()
end)
end
end
end)
end)
Sector1:AddToggle("Auto Chest Diamond",false,function(t)
   _G.AutoChestDiamond = t
end)

spawn(function()
pcall(function()
while wait() do
if _G.AutoChestDiamond then
pcall(function()
game:GetService("ReplicatedStorage").Remotes.BuyCrate:InvokeServer("Diamond")
end)
end
end
end)
end)
Sector1:AddToggle("Auto Chest Gold",false,function(t)
   _G.AutoChestGold = t
end)

spawn(function()
pcall(function()
while wait() do
if _G.AutoChestGold then
pcall(function()
game:GetService("ReplicatedStorage").Remotes.BuyCrate:InvokeServer("Gold")
end)
end
end
end)
end)
Sector1:AddToggle("Auto Chest Silver",false,function(t)
   _G.AutoChestSilver = t
end)

spawn(function()
pcall(function()
while wait() do
if _G.AutoChestSilver then
pcall(function()
game:GetService("ReplicatedStorage").Remotes.BuyCrate:InvokeServer("Silver")
end)
end
end
end)
end)
local Sector2 = Tab:CreateSector("Misc","Right")
 Sector2:AddLabel("EquipDisplay") Sector2:AddButton("Equip Heavenly Display",function()
  print("")game:GetService("ReplicatedStorage").Remotes.Equip:FireServer("Heavenly Display","Effects")
end)
Sector2:AddButton("Equip Darkness Display",function()
  print("")game:GetService("ReplicatedStorage").Remotes.Equip:FireServer("Darkness Incarnate","Effects")
end) Sector2:AddButton("Equip Ultra Instinct Display",function()
  print("")game:GetService("ReplicatedStorage").Remotes.Equip:FireServer("Ultra Instinct","Effects")
end) Sector3:AddSeperator("Op Script")
local Tab2 = CenterHubNo1:CreateTab("Config")
local config = library:ConfigSystem("MapName")
config:Load()
  while wait(.5) do
    pcall(function()
        local config = library:ConfigSystem("MapName")
        config:Save()
    end)
end
